{
  "name": "medistream-extension-dev",
  "displayName": "Medistream Development Tools (dev)",
  "description": "메디스트림에서 반복해서 사용하는 기능을 확장프로그램으로 분리했습니다.",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktseo41/medistream-extension.git"
  },
  "publisher": "Bohyeon",
  "contributes": {
    "commands": [
      {
        "command": "medistream-extension.startHotfix",
        "title": "[Medistream] Hotfix 시작"
      },
      {
        "command": "medistream-extension.cmg",
        "title": "[Medistream] Commit Message 생성"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}